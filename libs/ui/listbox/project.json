{
  "name": "listbox",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ui/listbox/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": ["type:ui", "scope:shared"],
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/ui/listbox/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "component-test": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "libs/ui/listbox/cypress.config.ts",
        "testingType": "component",
        "skipServe": true,
        "devServerTarget": "try-signals:build"
      }
    }
  }
}
